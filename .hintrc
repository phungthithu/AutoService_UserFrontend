{
  "extends": [
    "development"
  ],
  "hints": {
    "axe/forms": "off",
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "-webkit-text-size-adjust",
          "@-ms-viewport"
        ]
      }
    ],
    "axe/name-role-value": [
      "default",
      {
        "button-name": "off"
      }
    ],
    "axe/text-alternatives": [
      "default",
      {
        "image-alt": "off"
      }
    ],
    "axe/parsing": [
      "default",
      {
        "duplicate-id-aria": "off"
      }
    ],
    "meta-viewport": "off"
  },
  "browserslist": [
    "defaults",
    "not ie 11",
    "not and_chr <= 113",
    "not and_ff <= 113",
    "not chrome <= 113",
    "not edge <= 113",
    "not firefox <= 113",
    "not ios_saf <= 16.4",
    "not opera <= 98",
    "not safari <= 16.4",
    "not samsung <= 20"
  ]
}